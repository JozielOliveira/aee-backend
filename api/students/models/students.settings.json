{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "students",
  "info": {
    "name": "Students"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "students"
    },
    "student_responses": {
      "via": "student",
      "collection": "student-response"
    }
  }
}
